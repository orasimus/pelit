<% form_for @developer do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :country %><br />
    <%= f.text_field :country %>
  </p>

  <p>
    <%= f.submit "Save" %>
  </p>
<% end %>

<%= link_to "Developers", developers_path, :class => "green-link" %>
